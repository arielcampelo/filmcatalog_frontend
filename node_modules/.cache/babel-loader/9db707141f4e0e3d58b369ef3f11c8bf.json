UAAmC;;aAAf,KAAA,CAAA,IAAA,CAAK,Q,GAAQ,M;;AAAE,GADjE,E,IAAA,E;;AAAA,G,iBACkD,KAAA,CAAA,IAAA,CAAK,Q,GACrD,CAHN,C,EAII,W,EAMJ,mBAAA,CAEI,GAFJ,EAEI;AAFD,IAAA,IAAI,EAAC,GAEJ;AAFQ,IAAA,KAAK,EAAC,oCAEd;AAFmD,IAAA,EAAE,EAAG,aAExD;AAFuE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,SAAA,EAAF;AAAA,KAAA;AAE5E,GAFJ,EAA+F,SAA/F,C,CAhBJ,E;;AAAA,GA8BE,E,gBAVF,mBAAA,CAA+F,KAA/F,EAA+F;AAA1F,IAAA,KAAK,EAAC,qBAAoF;AAA9D,IAAA,IAAI,EAAC,OAAyD;AAAjD,IAAA,EAAE,EAAC;AAA8C,GAA/F,E,iBAAgF,KAAA,CAAA,O,CAAhF,EAAuF;AAAA;AAAvF,G,EAAuF,C,SAApB,KAAA,CAAA,S,CAAoB,C,CAUrF,EATF,WASE,EARF,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,C,gBADJ,mBAAA,CAA8F,KAA9F,EAAA,WAAA,EAA8F,IAA9F,EAA8F;AAAA;AAA9F,G,EAA8F,C,SAApB,KAAA,CAAA,W,CAAoB,C,CAC1F,CAFN,CAQE,EALF,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADF,mBAAA,CAA6E,GAA7E,EAA6E;AAA1E,IAAA,KAAK,EAAC,OAAoE;AAA5D,IAAA,IAAI,EAAC,GAAuD;AAAlD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAA,KAAA,CAAO,OAAP,CAAe,IAAf,CAAmB,WAAnB,CAAA;AAAA,KAAA;AAA6C,GAA7E,EAAkE,SAAlE,CACE,CAFN,CAKE,EAFF,mBAAA,CAAA,qEAAA,CAEE,EADF,WACE,CAlCN,CAmCE,CApCN,CAqCE,CAvCN,CAwCE,CA1CN,CA2CE,CA5CN,CA6CE,CA9CN,CA+CE,CAhDN,CAiDE,CAnDN,C","sourcesContent":["<template>\r\n<div class=\"container\">\r\n    <!-- Outer Row -->\r\n    <div class=\"row justify-content-center\">\r\n        <div class=\"col-xl-10 col-lg-12 col-md-9\">\r\n            <div class=\"card o-hidden border-0 shadow-lg my-5\">\r\n                <div class=\"card-body p-0\">\r\n                    <!-- Nested Row within Card Body -->\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-6 d-none d-lg-block bg-login-image\"></div>\r\n                        <div class=\"col-lg-6\">\r\n                            <div class=\"p-5\">\r\n                                <div class=\"text-center\" >\r\n                                    <h1 class=\"h4 text-gray-900 mb-4\" id=\"welcome\">Login</h1>\r\n                                </div>\r\n                                <form class=\"user\" action='#' method='pos' @submit.prevent=\"send_form()\">\r\n                                    <div class=\"form-group\">\r\n                                        <input type=\"email\" class=\"form-control form-control-user\"\r\n                                            id=\"email\" aria-describedby=\"emailHelp\"\r\n                                            placeholder=\"Type your e-mail addresss\" v-model=\"form.email\" >\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <input type=\"password\" class=\"form-control form-control-user\"\r\n                                            id=\"password\" placeholder=\"Password\" v-model=\"form.password\">\r\n                                    </div>\r\n                                        <div class=\"form-group\">\r\n                                        <div class=\"custom-control custom-checkbox small\">\r\n                                            <input type=\"checkbox\" class=\"custom-control-input\" id=\"customCheck\">\r\n                                            <label class=\"custom-control-label\" for=\"customCheck\">Remember</label>\r\n                                        </div>\r\n                                    </div>\r\n                                    <a href=\"#\" class=\"btn btn-primary btn-user btn-block\" id = \"loginButton\" @click=\"send_form()\">\r\n                                    Login\r\n                                    </a>\r\n                                </form>\r\n                                <div class=\"alert alert-warning\" role=\"alert\" id=\"warning\" v-show=\"showalert\">{{warning}}</div>\r\n                                <hr>\r\n                                <div align=\"center\">\r\n                                  <div class=\"spinner-border text-primary\"  role=\"status\" id=\"spin\" v-show=\"showspinner\" ></div>\r\n                                </div>\r\n                                <div class=\"text-center\">\r\n                                    <a class=\"small\" href=\"#\" @click=\"this.$router.push('/register')\">Sign up</a>\r\n                                </div>\r\n                                <!-- Use this div only if Login with Google And Facebook are disabled  -->\r\n                                <div style=\"margin-top:100px;\"> </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport Film from '../services/films'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data () {\r\n    return {\r\n      films: {},\r\n      form: {\r\n        email: 'jlopes@alumni.usp.br',\r\n        password: '123'\r\n      },\r\n      selected: true,\r\n      warning: '',\r\n      showalert: false,\r\n      showspinner: false\r\n    }\r\n  },\r\n  created () {\r\n\r\n  },\r\n  methods: {\r\n    send_form () {\r\n      if (this.check_form() === true) {\r\n        this.showspinner = true\r\n        Film.login(this.form).then(res => {\r\n          localStorage.setItem('token', res.data.token)\r\n          this.$router.push('/films')\r\n          console.log(res)\r\n        }).catch((error) => {\r\n          this.showalert = true\r\n          this.warning = error\r\n        }).finally(() => {\r\n          this.showspinner = false\r\n        })\r\n      }\r\n    },\r\n    clear_form () {\r\n      this.form.email = ''\r\n      this.form.password = ''\r\n    },\r\n    check_form () {\r\n      if (this.form.email === '') {\r\n        this.show_warning('E-mail is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      if (this.form.password === '') {\r\n        this.show_warning('Password is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    show_warning (message) {\r\n      this.warning = message\r\n      this.showalert = true\r\n    },\r\n    back2films () {\r\n      this.$router.push('/films')\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n@import url('https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i');\r\n@import '../assets/login/css/sb-admin-2.min.css';\r\n@import '../assets/login/css/custom.css';\r\n@import '../assets/login/vendor/fontawesome-free/css/all.min.css';\r\n.alert#warning{\r\n  margin-top:20px;\r\n  max-width: 500px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}