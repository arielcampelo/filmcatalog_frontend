AAL,GAAiB,IAAjB;AACA,UAAA,KAAI,CAAC,OAAL,GAAe,GAAG,CAAC,IAAJ,CAAS,OAAT,GAAmB,WAAnB,GAAiC,GAAG,CAAC,IAAJ,CAAS,KAAzD,CAJgC,CAKhC;AACD,SAND,EAMG,KANH,CAMS,UAAC,KAAD,EAAW;AAClB,UAAA,KAAI,CAAC,SAAL,GAAiB,IAAjB;AACA,UAAA,KAAI,CAAC,OAAL,GAAe,KAAf;AACD,SATD,EASG,OATH,CASW,YAAM;AACf,UAAA,KAAI,CAAC,WAAL,GAAmB,KAAnB;AACD,SAXD;AAYF;AACD,KAjBM;AAkBP,IAAA,UAlBO,wBAkBO;AACZ,WAAK,IAAL,CAAU,KAAV,GAAkB,EAAlB;AACA,WAAK,IAAL,CAAU,QAAV,GAAqB,EAArB;AACD,KArBM;AAsBP,IAAA,UAtBO,wBAsBO;AACZ,UAAI,KAAK,IAAL,CAAU,KAAV,KAAoB,EAAxB,EAA4B;AAC1B,aAAK,YAAL,CAAkB,sDAAlB;AACA,eAAO,KAAP;AACF;;AACA,UAAI,KAAK,IAAL,CAAU,QAAV,KAAuB,EAA3B,EAA+B;AAC7B,aAAK,YAAL,CAAkB,wDAAlB;AACA,eAAO,KAAP;AACF;;AACA,aAAO,IAAP;AACD,KAhCM;AAiCP,IAAA,YAjCO,wBAiCO,OAjCP,EAiCgB;AACrB,WAAK,OAAL,GAAe,OAAf;AACA,WAAK,SAAL,GAAiB,IAAjB;AACD,KApCM;AAqCP,IAAA,UArCO,wBAqCO;AACZ,WAAK,OAAL,CAAa,IAAb,CAAkB,QAAlB;AACF;AAvCO;AAjBI,CAAf","sourcesContent":["<template>\r\n  <div class='container-fluid'>\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-10 offset-sm-1\">\r\n        <!-- Title -->\r\n        <br><h2>Login</h2>\r\n        <!-- Form -->\r\n        <form action='#' method='pos' @submit.prevent=\"send_form()\">\r\n          <div class=\"mb-3\">\r\n            <label for=\"exampleInputEmail1\" class=\"form-label\" >Email address</label>\r\n            <input type=\"email\" class=\"form-control\" id=\"email\" aria-describedby=\"emailHelp\" style=\"max-width:350px;\" v-model=\"form.email\">\r\n          </div>\r\n          <div class=\"mb-3\">\r\n            <label for=\"exampleInputPassword1\" class=\"form-label\">Password</label>\r\n            <input type=\"password\" class=\"form-control\" id=\"password\" style=\"max-width:350px;\" v-model=\"form.password\">\r\n          </div>\r\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\r\n          <button type=\"button\" class=\"btn btn-primary\" id=\"clear\" @click=\"clear_form()\">Clear</button>\r\n        </form>\r\n        <!-- End Form -->\r\n        <div class=\"alert alert-warning\" role=\"alert\" id=\"warning\" v-show=\"showalert\">{{warning}}</div>\r\n        <div class=\"spinner-border text-primary\" role=\"status\" id=\"spin\" v-show=\"showspinner\" ></div>\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Film from '../services/films'\r\nimport Cookie from 'js-cookie'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      films: {},\r\n      form: {\r\n        email: 'jlopes@alumni.usp.br',\r\n        password: '123'\r\n      },\r\n      selected: true,\r\n      warning: '',\r\n      showalert: false,\r\n      showspinner: false\r\n    }\r\n  },\r\n  created () {\r\n    Cookie.remove('jwt')\r\n  },\r\n  methods: {\r\n    send_form () {\r\n      if (this.check_form() === true) {\r\n        this.showspinner = true\r\n        Film.login(this.form).then(res => {\r\n          Cookie.set('jwt', res.data.token)\r\n          this.$router.push('/films')\r\n          this.showalert = true\r\n          this.warning = res.data.message + '- Token: ' + res.data.token\r\n          // console.log(res)\r\n        }).catch((error) => {\r\n          this.showalert = true\r\n          this.warning = error\r\n        }).finally(() => {\r\n          this.showspinner = false\r\n        })\r\n      }\r\n    },\r\n    clear_form () {\r\n      this.form.email = ''\r\n      this.form.password = ''\r\n    },\r\n    check_form () {\r\n      if (this.form.email === '') {\r\n        this.show_warning('E-mail is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      if (this.form.password === '') {\r\n        this.show_warning('Password is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    show_warning (message) {\r\n      this.warning = message\r\n      this.showalert = true\r\n    },\r\n    back2films () {\r\n      this.$router.push('/films')\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.btn#clear{\r\n    margin-left:10px;\r\n  }\r\n  .alert#warning{\r\n    margin-top:20px;\r\n    max-width: 500px;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}