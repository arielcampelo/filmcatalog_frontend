d-lg-block bg-login-image\"></div>\r\n                        <div class=\"col-lg-6\">\r\n                            <div class=\"p-5\">\r\n                                <div class=\"text-center\" >\r\n                                    <h1 class=\"h4 text-gray-900 mb-4\" id=\"welcome\">Register</h1>\r\n                                </div>\r\n                                <form class=\"user\" action='#' method='pos' @submit.prevent=\"send_form()\">\r\n                                    <div class=\"form-group\">\r\n                                      <input type=\"name\" class=\"form-control form-control-user\"\r\n                                            id=\"name\" aria-describedby=\"emailHelp\"\r\n                                            placeholder=\"Type your name\" v-model=\"form.name\" >\r\n                                    </div>\r\n                                        <div class=\"form-group\">\r\n                                        <input type=\"email\" class=\"form-control form-control-user\"\r\n                                            id=\"email\" aria-describedby=\"emailHelp\"\r\n                                            placeholder=\"Type your e-mail addresss\" v-model=\"form.email\" >\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <input type=\"password\" class=\"form-control form-control-user\"\r\n                                            id=\"password\" placeholder=\"Password\" v-model=\"form.password\">\r\n                                    </div>\r\n                                        <div class=\"form-group\">\r\n                                        <div class=\"custom-control custom-checkbox small\">\r\n                                            <input type=\"checkbox\" class=\"custom-control-input\" id=\"customCheck\">\r\n                                            <label class=\"custom-control-label\" for=\"customCheck\">Remember</label>\r\n                                        </div>\r\n                                    </div>\r\n                                    <a href=\"#\" class=\"btn btn-primary btn-user btn-block\" id = \"loginButton\" @click=\"send_form()\">\r\n                                    Register\r\n                                    </a>\r\n                                </form>\r\n                                <div class=\"alert alert-warning\" role=\"alert\" id=\"warning\" v-show=\"showalert\">{{warning}}</div>\r\n                                <hr>\r\n                                <div align=\"center\">\r\n                                  <div class=\"spinner-border text-primary\"  role=\"status\" id=\"spin\" v-show=\"showspinner\" ></div>\r\n                                </div>\r\n                                <div class=\"text-center\">\r\n                                    <a class=\"small\" href=\"#\" @click=\"this.$router.push('/login')\">Login</a>\r\n                                </div>\r\n                                <!-- Use this div only if Login with Google And Facebook are disabled  -->\r\n                                <div style=\"margin-top:100px;\"> </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport Film from '../services/films'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data () {\r\n    return {\r\n      films: {},\r\n      form: {\r\n        name: '',\r\n        email: '',\r\n        password: ''\r\n      },\r\n      selected: true,\r\n      warning: '',\r\n      showalert: false,\r\n      showspinner: false\r\n    }\r\n  },\r\n  created () {\r\n\r\n  },\r\n  methods: {\r\n    send_form () {\r\n      if (this.check_form() === true) {\r\n        this.showspinner = true\r\n        Film.add_user(this.form).then(res => {\r\n          this.$router.push('/login')\r\n          console.log(res)\r\n        }).catch((error) => {\r\n          this.showalert = true\r\n          this.warning = error\r\n        }).finally(() => {\r\n          this.showspinner = false\r\n        })\r\n      }\r\n    },\r\n    clear_form () {\r\n      this.form.name = ''\r\n      this.form.email = ''\r\n      this.form.password = ''\r\n    },\r\n    check_form () {\r\n      if (this.form.name === '') {\r\n        this.show_warning('Name is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      if (this.form.email === '') {\r\n        this.show_warning('E-mail is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      if (this.form.password === '') {\r\n        this.show_warning('Password is empty. You must fill in all of the fields.')\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n    show_warning (message) {\r\n      this.warning = message\r\n      this.showalert = true\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n@import url('https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i');\r\n@import '../assets/login/css/sb-admin-2.min.css';\r\n@import '../assets/login/css/custom.css';\r\n@import '../assets/login/vendor/fontawesome-free/css/all.min.css';\r\n[v-cloak] {\r\n  display: none;\r\n}\r\n.alert#warning{\r\n  margin-top:20px;\r\n  max-width: 500px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}